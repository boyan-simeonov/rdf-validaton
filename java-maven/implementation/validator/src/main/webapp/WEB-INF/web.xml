<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>NLP2RDF</display-name>
    <servlet>
        <servlet-name>validator</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.validator.ValidateWS</servlet-class>
        
        <init-param>
            <param-name>logDirectory</param-name>
            <param-value>/RDFUnitLog</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>validator</servlet-name>
        <url-pattern>/validate</url-pattern>
    </servlet-mapping>

</web-app>
